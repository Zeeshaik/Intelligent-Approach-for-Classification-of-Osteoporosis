{
    "cleanUrls": true,
    "trailingSlash": false
    "version": 2,
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Flask App/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production"
    }
  }
  
